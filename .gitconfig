[user]
	name = Anton Panferov
	email = [EMAIL_HERE]
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/smaant/.gitignore
	ignorecase = false
	editor = vim

[alias]
	co = checkout
	ci = commit
	st = status 
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	back = reset --hard HEAD~1
[color]
	ui = auto
[credential]
	helper = osxkeychain
[push]
	default = simple
